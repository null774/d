				try {
					this.contextGL.texImage2D(this.contextGL.TEXTURE_2D, 0, this.contextGL.RGBA, this.contextGL.RGBA, this.contextGL.UNSIGNED_BYTE, e), this.contextGL.getError() != this.contextGL.NO_ERROR && (n = !0)
				} catch (r) {
					n = !0
				}
