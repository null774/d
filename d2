		};
		var W = {
				kind: "loop"
			},
			X = {
				kind: "switch"
			};
		z.isLet = function() {
			if (this.options.ecmaVersion < 6 || !this.isContextual("let")) return !1;
			N.lastIndex = this.pos;
			var e = N.exec(this.input),
				t = this.pos + e[0].length,
				n = this.input.charCodeAt(t);
			if (n === 91 || n === 123) return !0;
			if (h(n, !0)) {
