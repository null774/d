				return e.help = translate("Sell %d x %s!", [e.nr, e.name]), e.callback = function(e) {
					n.game.menu.closePopUpNow(), n.sellItem(t)
				}, e.text = "<font color=lime>$" + e.price + "</font> " + e.name + "s", e
			},
