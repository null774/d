		var q = I.prototype,
			R = /^(?:'((?:\\.|[^'])*?)'|"((?:\\.|[^"])*?)"|;)/;
		q.strictDirective = function(e) {
			var t = this;
			for (;;) {
				N.lastIndex = e, e += N.exec(t.input)[0].length;
				var n = R.exec(t.input.slice(e));
				if (!n) return !1;
				if ((n[1] || n[2]) === "use strict") return !0;
