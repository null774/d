			},
			showPlayerAdminOptions: function() {
				var e = "Admin Actions" + (this.playerprofiledata ? ": " + this.playerprofiledata.name : ""),
					t = this.showPopUp({
						title: e,
						hideclosebutton: !0,
						popupType: "adminactions"
					}),
					n = [],
					r = this;
				if (this.playerprofiledata) {
