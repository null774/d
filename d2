		I.prototype.isKeyword = function(t) {
			return this.keywords.test(t)
		}, I.prototype.isReservedWord = function(t) {
			return this.reservedWords.test(t)
		}, I.prototype.extend = function(t, n) {
			this[t] = n(this[t])
		}, I.prototype.loadPlugins = function(t) {
