			},
			sendMapMove: function(e, t, n) {
				this.sendMessage([Types.Messages.MAPMOVE, e, t, n])
			},
			sendAttack: function() {
				this.sendMessage([Types.Messages.ATTACK, -1])
			},
			sendHurt: function(e) {
