					r.game.menu.closePopUpNow("trade"), r.showTradeMenu(r.tradeinfo)
				})
			},
			updateTradeItem: function(e, t) {
				var n = parseInt(document.changeitemform.changeitemamount.value.trim());
				if (n < 0) return;
